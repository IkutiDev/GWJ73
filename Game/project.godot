; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot Wild Jam 73"
run/main_scene="res://sandbox.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0.152941, 0.203922, 0.305882, 1)
boot_splash/image="res://logos/GWJ-Full-Logo-tag-Orange2.png"
config/icon="res://logos/GWJ-Half-Logo-Orange.svg"
boot_splash/minimum_display_time=1000

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[layer_names]

2d_physics/layer_1="DO NOT USE"
2d_physics/layer_2="Ground"
2d_physics/layer_3="Player"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
